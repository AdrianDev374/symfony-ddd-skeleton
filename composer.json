{
    "type": "symfony-ddd-skeleton",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/yaml": "6.3.*"
    },
    "autoload": {
        "psr-4": {
            "AdrianDev374\\Apps\\Template\\": "apps/template/src",
            "AdrianDev374\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdrianDev374\\Tests\\": [
                "tests"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    }
}
